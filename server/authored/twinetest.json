[
    {
        "id": "twinetest",
        "start": {
            "actions": [
                []
            ],
            "event": "Thestart"
        },
        "events": [
            {
                "type": "button",
                "name": "Thestart",
                "id": "Thestart",
                "subscription": "/press",
                "onstart": {
                    "speech": [
                        {
                            "words": "Well here we all are and hoping for the best",
                            "voice": "Daniel",
                            "rate": "180",
                            "delay": "0"
                        }
                    ]
                },
                "rules": [
                    {
                        "rule": {
                            "operator": "equals",
                            "operand": "next"
                        },
                        "next": "morespeech",
                        "actions": []
                    }
                ],
                "data": [
                    "next"
                ]
            },
            {
                "type": "button",
                "name": "morespeech",
                "id": "morespeech",
                "subscription": "/press",
                "onstart": {
                    "actions": [
                        [
                            {
                                "action": "http://[speech]:9105/api/speech",
                                "params": {
                                    "body": {
                                        "speech": [
                                            {
                                                "words": "right I am going to say this immediately!"
                                            }
                                        ]
                                    }
                                },
                                "method": "POST",
                                "delay": 5000
                            }
                        ],
                        [
                            {
                                "action": "http://[lenovo]:9102/api/message",
                                "params": {
                                    "query": {
                                        "message": "and I will say this at the same time"
                                    }
                                },
                                "method": "GET",
                                "delay": 0
                            }
                        ]
                    ]
                },
                "rules": [],
                "data": []
            }
        ]
    }
]